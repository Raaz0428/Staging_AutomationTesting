{
    "default": {
        "dryRun": false,
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "paths": [
            "e2etests/test/features/login.feature"
        ],
        "require": [
            "e2etests/test/steps/**/*.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            ["json", "reports/cucumber_report.json"],
            ["junit", "reports/junit.xml"],
            ["html", "reports/html-formatter.html"]
        ]
    }
}
